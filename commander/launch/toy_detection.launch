<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <node name="my_web_server" pkg="web-server-for-csocuc-project" type="main.py" output="screen" respawn="true"/>
  <!-- Start simulation -->
  <include file="$(find sim_env)/launch/conveyor_belt.launch"/>
  <!-- Trigger spawner node -->
  <arg name="objects" default="toy"/>
  <node name="object_spawner" pkg="sim_env" args="$(arg objects)" type="object_spawner.py" respawn="true" output="screen"/>

  <node name="toy_classifiyer_x" pkg="commander" args="camera_x keras/model_x.h5 keras/labels.txt" type="toy_detection.py" output="screen" />
  <node name="image_publisher_x" pkg="commander" args="camera_x tf/x_ssd_mobnet_100k.tflite tf/labels.txt" type="image_publisher.py" output="screen"/>
 
</launch>
