<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <!-- Start simulation -->
  <include file="$(find sim_env)/launch/conveyor_belt.launch"/>
 <!-- Trigger spawner node -->
  <arg name="objects" default="toy"/>
  <node name="object_spawner" pkg="sim_env" args="$(arg objects)" type="object_spawner.py" respawn="true" output="screen"/>
  <!-- Start capture service-->
  <node name="image_capture" pkg="commander" type="image_capture.py" respawn="true" output="screen"/>

  <!-- Run node all detectors
  <node name="toy_classifiyer_x" pkg="commander" args="camera_x" type="toy_classifier.py" output="screen"/>
 -->
</launch>
